    "@antv/l7-maps": "^2.13.9",
    "@antv/l7-react": "^2.4.3",
    "@monaco-editor/react": "^4.4.6",
    "axios": "^1.3.3",
    "element-resize-detector": "^1.2.4",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "monaco": "^1.201704190613.0",
    "monaco-editor": "^0.36.1",
    "react": "17.x",
    "react-dom": "17.x",
    "swr": "^2.0.4",
    "umi": "^3.5.37",
